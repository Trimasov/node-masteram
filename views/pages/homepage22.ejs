</script><script src="/js/wow.min.js"></script>
<script>
    wow = new WOW(
        {
            boxClass:     'wow',      // default
            animateClass: 'animated', // default
            offset:       0,          // default
            mobile:       false,       // default
            live:         true        // default
        }
    )
    wow.init();


    let element = document.getElementById('phoneNumber');
    let maskOptions = {
        mask: '+{7}(000)000-00-00'
    };
    let mask = new IMask(element, maskOptions);

    let citylist = <%- JSON.stringify(sails.config.globals.citylist) %>;


    $("#area").change((function(){
        $(".area_selected").show(100);
        let first_option = $(this).find("option").first();
        if (first_option.text().length == 0){
            $(this).find("option").first().remove();
        }
        $("#topic").html("<option></option>")
        citylist[$(this).val()].forEach((city)=>{
            $("#topic").append(`<option value=\"${city}\">${city}</option>`)
        });
        $(".city_selected").hide(100);
    }));

    $("#topic").change(function(){
        $(".city_selected").show(100);
        let first_option = $(this).find("option").first();
        if (first_option.text().length == 0){
            $(this).find("option").first().remove();
        }
    })



    $("#sendOrder").click(() => {
        let formDiv = $("#step1");
        let error = false;

        if (formDiv.find("#description").val().length > 0){
            formDiv.find("#description").removeClass("has-error");
        }else{
            formDiv.find("#description").addClass("has-error");
            error = true;
        }

        if (formDiv.find("#phoneNumber").val().length > 10){
            formDiv.find("#phoneNumber").removeClass("has-error");
        }else{
            formDiv.find("#phoneNumber").addClass("has-error");
            error = true;
        }

        if (formDiv.find("#name").val().length > 0){
            formDiv.find("#name").removeClass("has-error");
        }else{
            formDiv.find("#name").addClass("has-error");
            error = true;
        }

        if (!error){
            data = {};
            $("#step1 form").serializeArray().forEach((l)=>{
                data[l.name] = l.value;
            })
            $("#step1").toggle();
            $("#step2").toggle();
            $.ajax({
                url: "/order/registerorder",
                type: "POST",
                data: data,
                success: (data)=>{
                    console.log(data);
                }
            })
        }
    })

    $(function(){
        $('#area option').first().attr("selected", true);
        $('#area option').trigger("change");
    })

</script>
